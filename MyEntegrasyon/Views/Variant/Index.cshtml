 @using MyEntegrasyon.Models.Myikas.VariantC
 @model List<ListVariantType>


<style>
    #customers {
        font-family: Arial, Helvetica, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

        #customers td, #customers th {
            border: 1px solid #ddd;
            padding: 8px;
        }

        #customers tr:nth-child(even) {
            background-color: #f2f2f2;
        }

        #customers tr:hover {
            background-color: #ddd;
        }

        #customers th {
            padding-top: 12px;
            padding-bottom: 12px;
            text-align: left;
            background-color: #04AA6D;
            color: white;
        }
</style>





<table id="customers">
    <thead>
        <tr>
            <th>deleted</th>
            <th>id</th>
            <th>name</th>
            <th>selectionType</th>
            <th>values</th>
          
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.deleted</td>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>@item.selectionType</td>
                <td>



                    <table>
                        <thead>
                            <tr>
                                <th>deleted</th>
                                <th>id</th>
                                <th>name</th>
                            </tr>
                        </thead>
                        <tbody>

                            @foreach (var item_values in item.values!)
                            {
                                <tr>
                                    <td>@item_values.deleted</td>
                                    <td>@item_values.id</td>
                                    <td>@item_values.name</td>
                                </tr>
                            }


                        </tbody>
                    </table>









                </td>
            </tr>
        }

       
    </tbody>
</table>
  
